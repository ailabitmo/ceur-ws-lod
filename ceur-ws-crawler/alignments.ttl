@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix dbpedia-owl: <http://dbpedia.org/ontology/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix timeline: <http://purl.org/NET/c4dm/timeline.owl#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix bibo: <http://purl.org/ontology/bibo/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix swrc: <http://swrc.ontoware.org/ontology#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix swc: <http://data.semanticweb.org/ns/swc/ontology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .


## Workshop ##
swrc:Workshop     owl:equivalentClass     bibo:Workshop .
swrc:eventTitle   owl:equivalentProperty  [ bibo:title , 
                                            bibo:shortTitle,
                                            dcterms:title ] . 
swrc:edition      owl:equivalentProperty  [ bibo:edition ,
                                            dcterms:hasVersion ].
swrc:atEvent      owl:equivalentProperty  bibo:presentedAt .
swrc:date         owl:equivalentProperty  [ bibo:date , dcterms:date ] .


## Proceedings ##
swrc:Proceedings    owl:equivalentClass     [ bibo:Proceedings ,
                                              foaf:Document ,
                                              dc:Text ] .
swrc:homepage       owl:equivalentProperty  foaf:homepage .
swrc:creationDate   owl:equivalentProperty  [ bibo:created ,
                                              dcterms:date ] .
swrc:editor         owl:equivalentProperty  [ bibo:editor ,
                                              foaf:maker ,
                                              dcterms:creator ] .
swrc:title          owl:equivalentProperty  [ bibo:title , # bibo:title is annotation property - should we remove this mapping?
                                              dcterms:title ,
                                              foaf:title ] .


## Paper ##
swrc:InProceedings  owl:equivalentClass     [ bibo:Article ,
                                              foaf:Document ,
                                              dc:Text ] .
swrc:creator        owl:equivalentProperty  [ bibo:contributor ,
                                              foaf:maker ,
                                              dcterms:creator ] .


## Person ##
swrc:Person   owl:equivalentClass     [ bibo:Person ,
                                        foaf:Person ,
                                        dcterms:Agent ] .
swrc:name     owl:equivalentProperty  [ bibo:name ,
                                        foaf:name ,
                                        dcterms:title ] .
